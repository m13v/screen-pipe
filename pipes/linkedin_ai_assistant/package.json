{
  "name": "linkedin_ai_assistant",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build-exe": "next build && pkg .",
    "build-dev-exe": "pkg dev-server.js --targets node18-macos-x64,node18-win-x64,node18-linux-x64 --output dist-exe/linkedin-ai-dev"
  },
  "bin": "server.js",
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ],
    "assets": [
      "package.json",
      "node_modules/**/*",
      "app/**/*",
      "components/**/*",
      "lib/**/*",
      "public/**/*"
    ],
    "targets": [
      "node18-macos-x64",
      "node18-win-x64",
      "node18-linux-x64"
    ],
    "outputPath": "dist-exe"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@screenpipe/js": "latest",
    "@sentry/nextjs": "^8.42.0",
    "@tauri-apps/api": "^2.1.1",
    "caniuse-lite": "^1.0.30001687",
    "chrome-launcher": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.13.1",
    "litellm": "^0.12.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.462.0",
    "next": "^15.0.4",
    "node-fetch": "^3.3.2",
    "pkg": "^5.8.1",
    "puppeteer-core": "^23.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.1.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "bun-types": "latest",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
